npts 60 60 60                        # num.grid points in xyz
parameter_file AD4.1_bound.dat       # force field default parameter file

gridfld 1ppc_rec.maps.fld            # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C CA HD N NA OA SA  # receptor atom types
ligand_types A C HD N NA OA S        # ligand atom types
receptor 1ppc_rec.pdbqt              # macromolecule
gridcenter -2.7456 5.4574 23.6688    # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 1ppc_rec.A.map                   # atom-specific affinity map
map 1ppc_rec.C.map                   # atom-specific affinity map
map 1ppc_rec.HD.map                  # atom-specific affinity map
map 1ppc_rec.N.map                   # atom-specific affinity map
map 1ppc_rec.NA.map                  # atom-specific affinity map
map 1ppc_rec.OA.map                  # atom-specific affinity map
map 1ppc_rec.S.map                   # atom-specific affinity map
elecmap 1ppc_rec.e.map               # electrostatic potential map
dsolvmap 1ppc_rec.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
